{
  "name": "@scratchworks/scratchworks-backend",
  "version": "1.0.0",
  "main": "index.ts",
  "types": "index.ts",
  "license": "MIT",
  "_moduleAliases": {
    "@db": "./build/db",
    "@router": "./build/trpc",
    "@utils": "./build/utils"
  },
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc",
    "db:deploy": "dotenv -e .env prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name",
    "dev": "dotenv -e .env nodemon",
    "image:build": "docker build . -t ozahnitko/scratchworks-backend-server:dev-latest",
    "image:run": "docker run -d --env-file .env --env NODE_ENV=prod --name scratchworks-backend-server --network container:postgres ozahnitko/scratchworks-backend-server:dev-latest",
    "services:start:dev": "docker compose -f docker-compose-dev.yml -p scratchworks-backend-dev up -d",
    "start:prod": "node ./build/index.js"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@trpc/server": "^10.7.0",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.18",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
